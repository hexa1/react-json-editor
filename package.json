    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.4.0",
    "eslint-config-hexa1": "github:hexa1/eslint-config-hexa1#v0.1.1",
    "eslint-import-resolver-webpack": "^0.4.0",
    "expect": "^1.20.2",
    "express": "^4.14.0",
